t(timeout);\n		timeout = setTimeout(onTimeout, delay);\n	}\n}\n\nfunction throttle(fn, delay) {\n	var running = false,\n	onTimeout = function() {\n		running = false;\n		fn.apply(this, arguments);\n	}\n\n	return function() {\n		if(running) return false;\n		running = true;\n		setTimeout(onTimeout, delay);\n	}\n}\n\nfunction location() {\n	return URL.parse(window.location.href, true);\n}",
